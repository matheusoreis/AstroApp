{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "lib": [
      "ES2023"
    ],
    "target": "ES2023",
    "module": "Node16",
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strictPropertyInitialization": false,
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@/components/*": [
        "./src/components/*"
      ],
      "@/entities/*": [
        "./src/domains/entities/*"
      ],
      "@/interfaces/*": [
        "./src/domains/interfaces/*"
      ],
      "@/services/*": [
        "./src/domains/services/*"
      ],
      "@/database/*": [
        "./src/infrastructures/database/*"
      ],
      "@/repositories/*": [
        "./src/infrastructures/repositories/*"
      ],
      "@/layouts/*": [
        "./src/layouts/*"
      ],
      "@/libs/*": [
        "./src/libs/*"
      ],
      "@/pages/*": [
        "./src/pages/*"
      ],
      "@/shared/*": [
        "./src/shared/*"
      ]
    },
    "verbatimModuleSyntax": true
  }
}